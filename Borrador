public class Usuario {
    private String nombreUsuario;
    private String correoElectronico;
    private String contraseña;
    private double saldo;

    public void registrarse(String nombreUsuario,String correoElectronico,String contraseña) {
	this.nombreUsuario=nombreUsusario;
	this.correoElectronico=correoElectronico;
	this.contraseña=contraseña;
		}
    public boolean iniciarSesion(String Usuario,String Correo,String Clave) {
	boolean flag=true;
	if (nombreUsuario!=Ususario || correoElectronico!=Correo || contraseña=Clave ){
		flag=false;
	}
	return flag
	}
